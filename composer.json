{
  "name": "makeandship/acf-elasticsearch",
  "description": "Integrate wordpress and advanced custom fields with elasticsearch.",
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "specialistpharmacyservice/logging-php",
        "version": "v1.0.1",
        "type": "library",
        "source": {
          "url": "https://github.com/SpecialistPharmacyService/logging-php.git",
          "type": "git",
          "reference": "v1.0.1"
        },
        "require": {
          "composer/installers": "^2.0"
        }
      }
    }
  ],
  "require": {
    "elasticsearch/elasticsearch": "7.11",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "jakub-onderka/php-console-highlighter": "^0.3.2",
    "ruflin/elastica": "^7.1.0",
    "php-http/httplug": "^1.1.0",
    "monolog/monolog": "^1.21.0",
    "specialistpharmacyservice/logging-php": "v1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "authors": [
    {
      "name": "mthomsit",
      "email": "mark@makeandship.co.uk"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
